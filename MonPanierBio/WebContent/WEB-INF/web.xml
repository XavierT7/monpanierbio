<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>MonPanierBio</display-name>

	<servlet>
		<servlet-name>AccueilServlet</servlet-name>
		<servlet-class>com.monpanierbio.servlets.AccueilServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AccueilServlet</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreerPanierServlet</servlet-name>
		<servlet-class>com.monpanierbio.servlets.CreerPanierServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CreerPanierServlet</servlet-name>
		<url-pattern>/creer-panier</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SubmitFormServlet</servlet-name>
		<servlet-class>com.monpanierbio.servlets.SubmitFormServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SubmitFormServlet</servlet-name>
		<url-pattern>/submitForm</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>